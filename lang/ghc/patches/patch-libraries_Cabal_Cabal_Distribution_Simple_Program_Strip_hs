     IOS -> return ()
     AIX -> return ()
     Solaris -> return ()
+    OpenBSD -> -- '--strip-unneeded' sometimes strips too much on OpenBSD.
+           return ()
     Windows -> -- Stripping triggers a bug in 'strip.exe' for
                -- libraries with lots identically named modules. See
