$OpenBSD: patch-src_dialogs_dialog-preferences_c,v 1.2 2010/04/17 06:56:05 ajacoutot Exp $

gcc3 doesn't like function names that end with a numeral right after
alphabetical characters.

--- src/dialogs/dialog-preferences.c.orig	Fri Mar 26 23:38:55 2010
+++ src/dialogs/dialog-preferences.c	Sat Apr 17 07:46:19 2010
@@ -377,7 +377,7 @@ int_pref_create_widget (GOConfNode *node, GtkWidget *t
 }
 
 static gboolean
-powerof2 (int i)
+powerof_2 (int i)
 {
 	return i > 0 && (i & (i - 1)) == 0;
 }
@@ -387,9 +387,9 @@ cb_power_of_2 (GtkAdjustment *adj)
 {
 	int val = (int)adj->value;
 
-	if (powerof2 (val - 1))
+	if (powerof_2 (val - 1))
 		gtk_adjustment_set_value (adj, (val - 1) * 2);
-	else if (powerof2 (val + 1))
+	else if (powerof_2 (val + 1))
 		gtk_adjustment_set_value (adj, (val + 1) / 2);
 }
 
