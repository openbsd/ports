bin/glib-config
@unexec install-info --delete %D/info/glib.info %D/info/dir
info/glib.info
@exec [ -f %D/info/dir ] || sed -ne '1,/Menu:/p' /usr/share/info/dir > %D/info/dir
@exec install-info %D/info/glib.info %D/info/dir
lib/libglib.so.1.2
lib/libglib.la
lib/libglib.a
lib/libgmodule.so.1.2
lib/libgmodule.la
lib/libgmodule.a
lib/libgthread.so.1.2
@exec /sbin/ldconfig -m %B
lib/libgthread.la
lib/libgthread.a
man/man1/glib-config.1
share/aclocal/glib.m4
include/glib/glib.h
include/glib/gmodule.h
include/glib/glibconfig.h
@dirrm include/glib
