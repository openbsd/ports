$OpenBSD: patch-t_t1450-fsck_sh,v 1.3 2017/07/14 04:54:23 benoit Exp $

Avoid GNU grep syntax

--- t/t1450-fsck.sh.orig	Thu Jul 13 00:59:17 2017
+++ t/t1450-fsck.sh	Fri Jul 14 06:44:54 2017
@@ -573,7 +573,7 @@
 		remove_object $(git rev-parse julius:caesar.t) &&
 		test_must_fail git fsck --name-objects >out &&
 		tree=$(git rev-parse --verify julius:) &&
-		egrep "$tree \((refs/heads/master|HEAD)@\{[0-9]*\}:" out
+		grep -E "$tree \(refs/heads/master|HEAD@{[0-9]*}:" out
 	)
 '
 
