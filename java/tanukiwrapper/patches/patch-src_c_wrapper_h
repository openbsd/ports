+#ifndef OPENBSD
 #include <sys/timeb.h>
+#else
+struct timeb {
+	time_t  time;                   /* seconds since the Epoch */
+	unsigned short millitm;         /* + milliseconds since the Epoch */
+	short   timezone;               /* minutes west of UTC */
