bin/wdiff
@unexec install-info --delete %D/info/wdiff.info %D/info/dir
info/wdiff.info
@exec [ -f %D/info/dir ] || sed -ne '1,/Menu:/p' /usr/share/info/dir > %D/info/dir
@exec install-info %D/info/wdiff.info %D/info/dir
