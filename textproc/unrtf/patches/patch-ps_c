-		x y fontAscent 1.2 mul add dup /y2 exch def moveto	\n\
-		x w add y2 lineto stroke				\n\
-	} if					\n\
-	/x x w add def  			\n\
-	/didBR false def 			\n\
-	/didShowPage false def 			\n\
