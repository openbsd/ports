minimo/run-minimo.sh
%%debug%%
@cwd ${LOCALBASE}/minimo
@exec cd %D && env HOME=/tmp LD_LIBRARY_PATH=%D ./regxpcom
@exec rm -rf /tmp/.mozilla
@unexec rm -f %D/components/compreg.dat
@unexec rm -f %D/components/xpti.dat
